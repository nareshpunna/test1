<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
        xmlns:form="sap.ui.commons.form"
        xmlns:layout="sap.ui.layout"
		controllerName="centerforsight.Registration" xmlns:html="http://www.w3.org/1999/xhtml"
		class="sapUiSizeCompact">
	<Page title="Center For Sight Registration" showNavButton="true" navButtonPress="onBack">
		<content>
	      <form:SimpleForm columnsL="1" columnsM="1" layout="ResponsiveGridLayout" editable="true">
	      <form:content>
	      	<core:Title text="Personal Inforamtion"/>
	      	<Label text="First Name"/>
	      	<Input>
	      	<layoutData>
	      	 <layout:GridData span="L3 M6 S12"/>
	      	</layoutData>
	      	</Input>
	      	<Label text="Middle Name"/>
	      	<Input>
	      	<layoutData>
	      	 <layout:GridData span="L3 M6 S12"/>
	      	</layoutData>
	      	</Input>
	      	<Label text="Last Name"/>
	      	<Input>
	      	<layoutData>
	      	 <layout:GridData span="L3 M6 S12"/>
	      	</layoutData>	      		      	
	      	</Input>
	      	<Label text="Gender"/>
	      	<RadioButton groupName="gender" text="Male">
	      	  <layoutData>
	      	    <layout:GridData span="L1 M6 S12"/>
	      	  </layoutData>
	      	</RadioButton>
	      	<RadioButton groupName="gender" text="Female">
	      	  <layoutData>
	      	    <layout:GridData span="L1 M6 S12"/>
	      	  </layoutData>	      	
	      	</RadioButton>
	      	<core:Title text="Address"/>
	      	<Label text="Street"/>
	      	<Input >
	      	<layoutData>
	      	 <layout:GridData span="L3 M6 S12"/>
	      	</layoutData>
	      	</Input>
	      	<Label text="City">
	      	</Label>
	      	<Input>
	      	  <layoutData>
	      	   <layout:GridData span="L3 M6 S12"/>
	      	  </layoutData>
	      	</Input>
	      	<Label text="Country"></Label>
	      	<Input>
	      	<layoutData>
	      	<layout:GridData sapn="L3 M6 S12"></layout:GridData>
	      	</layoutData>
	      	</Input>
	      </form:content>
	      </form:SimpleForm>
		</content>
	</Page>
</core:View>